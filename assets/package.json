{
  "name": "@cyia/localize",
  "version": "1.3.1",
  "description": "Angular 本地化独立版本;Angular localize standalone version",
  "keywords": [
    "i18n",
    "l10n",
    "internationalization",
    "localize",
    "translate",
    "angular",
    "localization",
    "language"
  ],
  "bin": {
    "i18n": "./bin/index.js",
    "l10n": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wszgrcy/cyia-localize.git"
  },
  "bugs": {
    "url": "https://github.com/wszgrcy/cyia-localize/issues"
  },
  "homepage": "https://github.com/wszgrcy/cyia-localize#readme",
  "author": "wszgrcy",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./fesm2022/index.mjs",
      "require": "./commonjs/index.js",
      "default": "./commonjs/index.js",
      "esm": "./fesm2022/index.mjs"
    },
    "./cli": {
      "types": "./cli/index.d.ts",
      "import": "./cli/index.mjs",
      "require": "./cli/index.js",
      "default": "./cli/index.js",
      "esm": "./cli/index.mjs"
    }
  },
  "dependencies": {
    "@code-recycle/cli": "1.3.10",
    "commander": "12.0.0",
    "yaml": "2.4.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}